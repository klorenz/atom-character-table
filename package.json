{
  "name": "character-table",
  "main": "./lib/character-table",
  "version": "0.4.3",
  "description": "Insert a character from a (almost) complete Unicode Character Table or use vim digraph style to insert it.",
  "repository": "https://github.com/klorenz/atom-character-table",
  "license": "MIT",
  "engines": {
    "atom": ">=0.174.0 <2.0.0"
  },
  "keywords": [
    "character",
    "table",
    "special",
    "unicode",
    "symbol",
    "glyph",
    "mnemonic",
    "digraph"
  ],
  "activationCommands": {
    "atom-workspace": [
      "character-table:toggle",
      "character-table:show-enabled-mnemonics"
    ]
  },
  "dependencies": {
    "atom-space-pen-views": ">=2.0.0",
    "fuzzaldrin": "^2.1.0",
    "q": "^1.0.1"
  }
}
